{
  "compilerOptions": {
    "module": "esnext",
    "jsx": "react",
    "moduleResolution": "node",
    "target": "esnext",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/types/*": ["types/*"],
      "@/styles/*": ["styles/*"],
      "@/utils/*": ["utils/*"]
    },
    "incremental": true
  }
}
